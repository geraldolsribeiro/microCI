\definecolor{StefaniniMidnightBlue}{RGB}{11,22,65}
\definecolor{StefaniniSpotlightBlue}{RGB}{50,70,145}
\definecolor{StefaniniGrayUnic}{RGB}{102,102,102}
\definecolor{StefaniniWhite}{RGB}{255,255,255}
\definecolor{StefaniniTech1}{RGB}{0,107,201}
\definecolor{StefaniniTech2}{RGB}{0,159,215}
\mode<presentation>
\setbeamercolor{normal text}{bg=white,fg=StefaniniSpotlightBlue}
\setbeamercolor{StefaniniUcture}{fg=strspotlightBlue}
\setbeamercolor{palette primary}{bg=StefaniniMidnightBlue,fg=white}
\setbeamercolor{palette secondary}{fg=black,bg=StefaniniGrayUnic}
\setbeamercolor{palette tertiary}{fg=StefaniniGrayUnic!75,bg=black}
\setbeamercolor{palette quaternary}{fg=white,bg=StefaniniSpotlightBlue}
\setbeamercolor{titlelike}{parent=palette quaternary}
\setbeamercolor{block title}{fg=white,bg=StefaniniTech1}
\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!10!bg}
\setbeamercolor{block title alerted}{use=alerted text,fg=black,bg=alerted text.fg!75!bg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!25!bg}
\setbeamercolor{block title example}{use=example text,fg=black,bg=example text.fg!75!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!25!bg}
\setbeamercolor{separation line}{fg=red}
\setbeamercolor{fine separation line}{}
\mode
<all>
